{
  "multipart": [
    {
      "when": {
        "active": "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_center"
      }
    },
    {
      "when": {
        "active": "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_center_active"
      }
    },
    {
      "when": {
        "up": "true",
        "active" : "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true",
        "active" : "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side_active",
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true",
        "active" : "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side",
        "uvlock": false,
        "x": 180
      }
    },
    {
      "when": {
        "down": "true",
        "active" : "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side_active",
        "uvlock": false,
        "x": 180
      }
    },
    {
      "when": {
        "north": "true",
        "active" : "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side",
        "uvlock": false,
        "x": 90
      }
    },
    {
      "when": {
        "north": "true",
        "active" : "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side_active",
        "uvlock": false,
        "x": 90
      }
    },
    {
      "when": {
        "east": "true",
        "active" : "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side",
        "y": 90,
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true",
        "active" : "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side_active",
        "y": 90,
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true",
        "active" : "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side",
        "y": 180,
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true",
        "active" : "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side_active",
        "y": 180,
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true",
        "active" : "false"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side",
        "y": 270,
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true",
        "active" : "true"
      },
      "apply": {
        "model": "riscj_blockits:block/bus_block_side_active",
        "y": 270,
        "x": 90,
        "uvlock": false
      }
    }
  ]
}